{
  "name": "sparrow-css",
  "version": "0.2.0",
  "description": "set of styles for web application, code by postcss",
  "engines": {
    "node": ">8.9.0"
  },
  "main": "src/index.pcss",
  "scripts": {
    "build": "postcss src/index.pcss -o dist/sparrow-ui.css --env production",
    "watch": "postcss src/index.pcss -o dist/sparrow-ui.css -w --env development"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheap-pets/sparrow-css.git"
  },
  "keywords": [
    "sparrow-ui",
    "sparrow-css"
  ],
  "author": "scrollbar-ww",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheap-pets/sparrow-css/issues"
  },
  "homepage": "https://github.com/cheap-pets/sparrow-css#readme",
  "devDependencies": {
    "@ant-design/colors": "^2.0.4",
    "autoprefixer": "^9.4.7",
    "postcss": "7.0.14",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.1.1",
    "postcss-conditionals": "^2.1.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-mix-color": "^0.1.6",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-selector-not": "^4.0.0",
    "postcss-simple-vars": "^5.0.1",
    "postcss-unprefix": "^2.1.4"
  }
}
